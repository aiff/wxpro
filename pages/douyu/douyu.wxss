.body{
    display: flex;
    flex-wrap:  wrap;
}

.cell{
    display: flex;
    flex-direction: column;
    font-size: 28rpx;
    align-items: center;
    width: 22.5%;
    margin-left: 2%;
    margin-top: 25rpx;
}

.icon{
    width: 168.75rpx;
    height: 168.75rpx;
    margin-bottom: 10rpx;
    border-radius: 6rpx;
}

.title{
    white-space: nowrap;
    overflow: hidden;
    text-overflow: ellipsis;
    max-width: 100%;
}